<h1>Editing Listing</h1>
<br>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for @listing, url: {action: "update"}, method: :patch do |l| %>

        <%= l.label :available %>
        <%= l.text_field :available %>
        <br>
        <%= l.label :price %>
        <%= l.text_field :price_per_hour %>
        <br><br>
        <%= l.submit "Save changes", class: "btn btn-primary" %>
        <% end %>

    </div>
</div>